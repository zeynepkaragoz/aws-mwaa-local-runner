## Airflow - optional ##
# AIRFLOW_UID=1000
# AIRFLOW_UI_ADMIN_USER=airflow
# Secret of the airflow OIDC client (in Keycloak)
AIRFLOW_OIDC_CLIENT_SECRET=TU4MRbdt9mLWMXWkQXQsZoDecJEkpO5q
AIRFLOW_OIDC_CLIENT_ID=airflow
AIRFLOW_KEYCLOAK_INTERNAL_HOST_ISSUER=http://keycloak:8080/realms/cbio
AIRFLOW_KEYCLOAK_PUBLIC_HOST_ISSUER=https://localhost-alt/auth/realms/cbio

## Airflow email configuration - optional ##
AIRFLOW_EMAIL_BACKEND=airflow.utils.email.send_email_smtp
AIRFLOW_SMTP_HOST=
AIRFLOW_SMTP_START_TLS=true
AIRFLOW_SMTP_SSL=false
AIRFLOW_SMTP_USERNAME=
# !!! Use App passwords to avoid using your actual password in this file (https://security.google.com/settings/security/apppasswords)
AIRFLOW_SMTP_PASSWORD=
AIRFLOW_SMTP_DEFAULT_SENDER=
# Port: TLS -> 587, SSL -> 465
AIRFLOW_SMTP_PORT = 587
# Airflow path to email templates, absolute paths inside containers
AIRFLOW_EMAIL_SUBJECT_TEMPLATE=/opt/airflow/dags/config/email_subject_failedtask.txt
AIRFLOW_EMAIL_CONTENT_TEMPLATE=/opt/airflow/dags/config/email_template_failedtask.txt

## Airflow transformation and DAGs - optional ##
# Path of the cbioportal-automation repository in the host machine
# AIRFLOW_VAR_HOST_HOME=/home/user/GitHub/cbioportal-automation
# AIRFLOW_DAG_DIR=/home/user/dags/
# DATAFILES_IN_HOST_DIR=/home/user/input_data/
# DATAFILES_OUT_HOST_DIR=/home/user/output_data/

# Link to folder that contains web resources for custom tab or custom js
# CUSTOM_JS_FRONTEND_RESOURCES=./client/custom-js
# CUSTOM_JS_FRONTEND_CONFIG=./client/custom-js/frontendConfig.json

## Digital Slide Archive - optional ##
# DSA_USER=0:0
# DSA_LOG_LOCATION=../logs/dsa

## Other customizations - optional ##
# CUSTOM_LOGO=./client/custom_logo.png
# CUSTOM_QUERY_GENE_LISTS=./client/custom_query_gene_lists.json

# Link to file that contains html/javascript snippet that will [required: index-page-include]
# be integrated into the cbioportal index.html file
# INDEX_PAGE_INCLUDE=

# HTML files to display under the "What's New?" and "Example Queries" headers in the right nav bar
# !!! Make sure to use the files below as skeleton in order to maintain the proper CSS formatting
# WHATSNEW_HTML=./client/whatsnew.html
# QUERIES_HTML=./client/queries.html

